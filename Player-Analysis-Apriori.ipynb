    "        if(combinePlayers[combo] not in count_combo):\n",
    "            count_combo[combinePlayers[combo]]=0\n",
    "        count_combo[combinePlayers[combo]]+=1\n",
    "    \n",
    "    return(count_combo)    "
   ]
