    "def unpacking_tuple(arr, arr2, arr3):\n",
    "    unpack=[]\n",
    "    \n",
    "    for i in arr:\n",
    "        f, s = i\n",
    "        if f not in unpack:\n",
    "            unpack.append(f)\n",
    "        if s not in unpack:\n",
    "            unpack.append(s)\n",
    "        \n",
    "            \n",
    "    ind=[]\n",
    "    for val in range(len(unpack)):\n",
    "        if(unpack[val] in arr2):\n",
    "            ind1=arr2.index(unpack[val])\n",
    "            ind.append(arr3[ind1]) \n",
    "    \n",
    "\n",
    "    return(unpack,ind)"
   ]
