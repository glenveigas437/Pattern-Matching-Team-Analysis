    "def support_count(countContribution, noOfGoals):\n",
    "    support=[]\n",
    "    for vals in countContribution:\n",
    "        val=round((countContribution[vals]/noOfGoals)*100, 2)\n",
    "        support.append(val)\n",
    "\n",
    "    return support    "
   ]
