    "def countContributions(arr):\n",
    "    countContribution={}\n",
    "    for val in arr:\n",
    "        for vals in val :\n",
    "            if(vals not in countContribution):\n",
    "                countContribution[vals]=0\n",
    "            countContribution[vals]+=1\n",
    "            \n",
    "        \n",
    "    return countContribution        "
