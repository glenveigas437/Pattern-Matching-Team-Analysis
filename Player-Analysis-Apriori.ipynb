    "def combo_find(playerPartnership, Goals):    \n",
    "    combinePlayers = []\n",
    "    iteration = 0\n",
    "    for player in playerPartnership:\n",
    "        for g in Goals:\n",
    "            iteration = 0\n",
    "            ids=[0,0]\n",
    "            i=0\n",
    "            for p in player:\n",
    "                if p in g:\n",
    "                    iteration+=1\n",
    "                    ids[i]=(g.index(p))\n",
    "                    i+=1\n",
    "            if (ids[1]==ids[0]+1 and iteration == len(player)):\n",
    "                combinePlayers.append(player)\n",
    "   \n",
    "    return(combinePlayers)"
   ]
  },
